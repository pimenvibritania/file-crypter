{
    "name": "pimenvibritania/file-crypter",
    "description": "Encrypt and decrypt any files with AES & Blowfish encryption for PHP (Laravel) project",
    "keywords": [
        "laravel", "encrypt", "decrypt", "encryption", "decryption",
        "cbc", "php", "file", "file-crypter", "aes", "bluefish", "hashing"
    ],
    "homepage": "https://github.com/pimenvibritania/file-crypter",
    "type": "library",
    "authors": [
        {
            "name": "pimenvibritania",
            "email": "pimenvibritania@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "8.*"
    },
    "require-dev": {
        "phpro/grumphp": "^0.22.0"
    },
    "autoload": {
        "psr-4": {
            "pimenvibritania\\FileCrypter\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "pimenvibritania\\FileCrypter\\FileCrypterServiceProvider"
            ],
            "aliases": {
                "FileCrypter": "FileCrypter"
            }
        }
    }
}
